  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qavajs/steps-playwright/issues"
  },
  "homepage": "https://github.com/qavajs/steps-playwright#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@qavajs/cli": "^0.35.0",
    "@qavajs/console-formatter": "^0.7.1",
    "@qavajs/html-formatter": "^0.15.4",
    "@qavajs/memory": "^1.7.0",
    "@qavajs/po-playwright": "^0.13.0",
    "@qavajs/webstorm-adapter": "^8.0.0",
    "@types/chai": "^4.3.12",
    "@types/express": "^4.17.21",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "electron": "^27.1.3",
    "express": "^4.18.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@playwright/test": "^1.42.1",
    "@qavajs/validation": "^0.7.1"
  }
