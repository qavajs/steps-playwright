  "devDependencies": {
    "@cucumber/cucumber": "^10.0.0",
    "@qavajs/cli": "^0.30.1",
    "@qavajs/console-formatter": "^0.6.0",
    "@qavajs/html-formatter": "^0.14.3",
    "@qavajs/memory": "^1.6.2",
    "@qavajs/po-playwright": "^0.11.1",
